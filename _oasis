  Path:        src
  Findlibname: facebook
  Modules:     Api, Auth, Types, Types_ext, Util
  Pack:        true
  BuildDepends:
    atdgen,
    bolt,
    calendar,
    cohttp,
    cohttp.lwt,
    uri,
    yojson
    
